{
  "name": "golda",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config ./projects/engine/webpack.server.config.js --progress --colors",
    "serve:ssr": "nodemon dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run docs:server:production",
    "build-serve:ssr": "npm run build:ssr && npm run serve:ssr",
    "watch:server": "ng run docs:server --watch",
    "watch:client": "ng build --watch",
    "watch:engine": "wait-on ./dist/server/main.js && nodemon --watch projects/ -e html,js,scss,css,ts --exec npm run compile:server",
    "watch": "rimraf dist && concurrently -k -n \"CLIENT,SERVER,ENGINE\" -c \"blue,green,yellow\" \"npm run watch:client\" \"npm run watch:server\" \"npm run watch:engine\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/platform-server": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@datorama/akita": "^1.23.1",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26",
    "apollo-angular": "^1.5.0-rc.0",
    "apollo-angular-link-http": "^1.3.1",
    "apollo-link": "^1.2.0",
    "apollo-client": "^2.4.0",
    "apollo-cache-inmemory": "^1.3.2",
    "graphql-tag": "^2.10.0",
    "graphql": "^0.13.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "graphiql": "^0.12.0",
    "isomorphic-fetch": "^2.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "wait-on": "^3.2.0",
    "webpack-cli": "^3.1.0"
  }
}
